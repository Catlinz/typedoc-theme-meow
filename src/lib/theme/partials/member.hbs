{{#if (shouldShowMemberTitle) ~}}
    {{{ memberTitle }}}
{{~/if}}

{{#if signatures ~}}
    {{#each signatures ~}}
        {{{signature 3}}}
        {{~br 2 ~}}
    {{~/each ~}}
{{else ~}}
    {{#if hasGetterOrSetter ~}}
        {{~> member.getterSetter}}
    {{~else ~}}
        {{{declaration 3}}}
        {{~br 2 ~}}
    {{~/if ~}}
{{/if}}

{{#each groups ~}}
    {{#each children ~}}
        {{#unless hasOwnDocument ~}}
            {{#ifIsLiteralType true ~}}
                {{declaration 3}}
                {{~br 2 ~}}
                {{{objectLiteral 3}}}
                {{~br 2 ~}}
            {{~/ifIsLiteralType ~}}

            {{#ifIsLiteralType false ~}}
                {{~> member hideHr=true}}
            {{~/ifIsLiteralType ~}}
        {{/unless ~}}
    {{/each ~}}
{{/each ~}}

{{#unless @last ~}}
    {{#unless hideHr ~}}___{{/unless ~}}
{{/unless}}